{
  "name": "@steamspeak/server",
  "version": "0.6.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn run check-sum && cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "yarn run check-sum && cross-env NODE_ENV=development node dist/index.js",
    "dev": "tsc-watch --project . --outDir ./dist --onSuccess \"yarn run start:dev\"",
    "check-sum": "cd ../../ && make check-ts",
    "prebuild": "cd ../../ && make check-config",
    "build": "tsc --build tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "plugin:refresh": "tsc -p tsconfig-plugin.json && tscpaths -p tsconfig-plugin.json -s src -o dist",
    "test": "jest tests/ --runInBand",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "author": {
    "name": "DalexHD",
    "email": "eldalexhd@gmail.com",
    "url": "https://github.com/dalexhd"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "chokidar": "^3.3.1",
    "cors": "^2.8.5",
    "eventemitter2": "^6.3.1",
    "express": "^4.17.1",
    "globaloffensive": "^2.0.2",
    "ioredis": "^4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mock-require": "^3.0.3",
    "moment": "^2.24.0",
    "mongoose": "^5.9.6",
    "should": "^13.2.3",
    "sinon": "^9.0.1",
    "steam": "1.4.1",
    "steam-totp": "^2.1.1",
    "steam-user": "^4.13.2",
    "steamcommunity": "^3.41.3",
    "steamid": "^1.1.3",
    "ts3-nodejs-library": "^2.4.1"
  },
  "bugs": {
    "url": "https://github.com/dalexhd/SteamSpeak/issues"
  },
  "homepage": "https://github.com/dalexhd/SteamSpeak#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^2.1.3",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.5.43",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^25.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
